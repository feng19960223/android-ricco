<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".BusinessActivity" >

    <include
        android:id="@+id/a1"
        android:layout_width="match_parent"
        android:layout_height="64dp"
        layout="@layout/include_business_title" />

    <include
        android:id="@+id/a2"
        android:layout_width="match_parent"
        android:layout_height="64dp"
        android:layout_below="@+id/a1"
        layout="@layout/include_business_tab_select" />

    <ListView
        android:id="@+id/listView_cate"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/a2" >
    </ListView>

    <FrameLayout
        android:id="@+id/frameLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/a2"
        android:background="#66AFAFAF"
        android:visibility="gone" >

        <include layout="@layout/include_business_city" />
    </FrameLayout>

</RelativeLayout>